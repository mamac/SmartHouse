
{% extends "LljmHomeBundle::layout.html.twig" %}

{% block title %}
  Delete sub zone - {{ parent() }}
{% endblock %}

{% block lljm_home_body %}

  <h2>{{ 'Delete a sub zone' | trans }}</h2>

  <p>
    {{ 'Confirm you want to delete sub zone' | trans }} : {{ subzone.subzonename }}.
  </p>

  <form action="{{ path('home_subzonedel', {'id': subzone.id}) }}" method="post">
    <a href="{{ path('home_subzone')}}" class="btn btn-default glyphicon glyphicon-chevron-left">
      Back
    </a>
    <input type="submit" value="Delete" class="btn btn-danger" />
    {{ form_rest(form) }}
  </form>

{% endblock %}